<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.efoodie">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Efoodie">


        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Efoodie">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.upload.UploadActivity"
            android:label="@string/menu_upload"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.Efoodie.ActionBar" />

        <activity
            android:name=".ui.explore.FiltersActivity"
            android:label="@string/filters"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.Efoodie.ActionBar">

        </activity>


    </application>

</manifest>